<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChatroomLayoutController">
   <children>
      <AnchorPane layoutY="262.0" prefHeight="42.0" prefWidth="700.0">
         <children>
            <Button fx:id="btnBrowsing" layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onAction="#handleBtnBrowsing" text="Browsing" />
            <Text layoutX="110.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter a message:">
               <font>
                  <Font size="18.0" />
               </font></Text>
            <TextField fx:id="tfEnterMessage" layoutX="262.0" layoutY="9.0" onAction="#handleEnterMessage" prefHeight="23.0" prefWidth="424.0" />
         </children>
      </AnchorPane>
      <ScrollPane fx:id="messagePane" prefHeight="264.0" prefWidth="700.0" />
   </children>
</AnchorPane>
